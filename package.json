{
  "name": "foodoasis-la",
  "version": "1.0.0",
  "description": "A map of healthy food locations",
  "author": "Jim Thoburn <jim@jimthoburn.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "minify-community-garden": "uglifyjs ./data/community-garden.js -o ./data/community-garden.min.js",
    "minify-farmers-market": "uglifyjs ./data/farmers-market.js -o ./data/farmers-market.min.js",
    "minify-food-pantry": "uglifyjs ./data/food-pantry.js -o ./data/food-pantry.min.js",
    "minify-summer-lunch": "uglifyjs ./data/summer-lunch.js -o ./data/summer-lunch.min.js",
    "minify-supermarket": "uglifyjs ./data/supermarket.js -o ./data/supermarket.min.js",
    "minify-data": "npm run minify-community-garden && npm run minify-farmers-market && npm run minify-food-pantry && npm run minify-summer-lunch && npm run minify-supermarket",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^4.4.0",
    "nuxt": "^1.0.0-rc11"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "uglify-es": "^3.1.9"
  }
}
