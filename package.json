{
  "name": "foodoasis-la",
  "version": "1.0.0",
  "description": "A map of healthy food locations",
  "author": "Jim Thoburn <jim@jimthoburn.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "nightwatch ./test/test.js --config ./test/nightwatch.config.js",
    "minify:locations": "uglifyjs ./data/locations.js -o ./data/locations.min.js",
    "minify:community-garden": "uglifyjs ./data/community-garden.js -o ./data/community-garden.min.js",
    "minify:farmers-market": "uglifyjs ./data/farmers-market.js -o ./data/farmers-market.min.js",
    "minify:food-pantry": "uglifyjs ./data/food-pantry.js -o ./data/food-pantry.min.js",
    "minify:summer-lunch": "uglifyjs ./data/summer-lunch.js -o ./data/summer-lunch.min.js",
    "minify:supermarket": "uglifyjs ./data/supermarket.js -o ./data/supermarket.min.js",
    "minify:data": "npm run minify:locations && npm run minify:community-garden && npm run minify:farmers-market && npm run minify:food-pantry && npm run minify:summer-lunch && npm run minify:supermarket",
    "minify:fix-data": "npm run minify:data && eslint --fix ./data/ --ext .min.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/pwa": "^2.6.0",
    "chromedriver": "^78.0.1",
    "cross-env": "^5.2.1",
    "nuxt": "^2.15.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "nightwatch": "^1.5.1",
    "nodemon": "^1.19.4",
    "prettier": "1.14.3",
    "selenium-server": "^3.141.59",
    "uglify-es": "^3.3.10"
  }
}
